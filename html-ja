        <meta charset="utf-8">
        <title>Sample_googleMap</title>

        <style>
            html {height: 100%;}
            html {height: 100%;}
            html {height: 100%; width: 100%;}
        </style>
    </head>

    <body>
        <div id="map"></div>
        <script src="http://maps.google.com/maps/api/js?key{apiキー}&language=ja"></script>

        <script>
            var MyLatLag = new google.maps.Lating(35.6811673, 139.7670516);
            var Options = {
                zoom: 15,
                center: MyLatLag,
                mapTypeId: "roadmap"
            };
            var map = new google.maps.Map(document.getElementById("map"), Options);
        </script>
    </body>
</html>
